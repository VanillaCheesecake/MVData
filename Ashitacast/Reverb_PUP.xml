	</if>
	   
    <if buffactive="11|12|21|128|129|130|131|132|133|134|135|136|137|138|139|140|141|142|144|145|146|147|148|149|167|168|174|175|189|404|557|558|559|560|561|562|563|564|567"> <!-- Bind|Weight|Addle|Burn|Frost|Choke|Rasp|Shock|Drown|Dia|Bio|STR/DEX/VIT/AGI/INT/MND/CHR Down|Max HP/MP/Accuracy/Attack/Evasion/Defence Down|Magic Defence Down|Magic Accuracy Down|Magic Attack Down|Max TP Down|Magic Evasion Down|GEO Versions of Stat Downs -->
		<command delay="150">/item "Panacea" &lt;me&gt;</command>
    </if>
	
	</if> <!-- Do not touch this. Handles item use. -->
	
	<!-- END OF ITEM USAGE SYNTAX -->

	<!-- Auto WS use. -->
		
	<if advanced="$Autows=1">
		<if p_status="engaged">
			<if p_tp=">999">
				<if t_distance="<4.5">
			<command>/ws "$ws" &lt;t&gt;</command>
			</if>
			</if>
		</if>
	</if>
	
