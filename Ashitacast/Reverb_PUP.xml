	<setvar name="Heroics" value="1" />
	</if>
	
	<else>
	<setvar name="Heroics" value="0" />
	</else>
	
	<if e_area="Heaven*|Metalworks|Chateau*|Ru'Lude*|Nashmau|Tavnazian Safehold|Port*|Bastok*|Windurst*|*San d'Oria|Southern San d'Oria*|Bastok Markets*|Windurst Waters*|*Jeuno|*Whitegate|*Adoulin|Selbina|Mhaura|Norg|Rabao|Kazham|*Memorial Library|Leafallia|Mog*"> <!-- Town zone Check -->
	<setvar name="Town" value="1" />
	</if>
	
	<else>
	<setvar name="Town" value="0" />
	</else>
	
<if p_status="engaged"><equip set="Default" /></if> <!--Forces idle/DT gear upon engagement-->
		
		<if advanced="$Town=0"><equip set="Default" /></if> <!--Idle defensive gear when standing still-->
		<else><equip set="Idletown" /></else>
		
		
		<if p_ismoving="True"> <!--Checks for movement since last outgoing 0x15(position update packet)-->
		
		<if p_status="engaged"> <!--Checks for movement while engaged, if true, keep the idle/DT feet on for obvious reasons-->
			<if advanced="$AutoTH=1">
				<if advanced="$FirstTH=0">
				
					<if t_distance="<4.5">
					<equip set="TH" /><gearlock length="3" />
					<setvar name="FirstTH" value="1" />
					</if>
				</if>
			</if>
			<else><equip set="Default" /></else>
		</if>
			
		<else><equip set="Movement" /></else> <!--If not in battle, equip movement feet-->
		</if>
	
	<if p_status="idle"><setvar name="FirstTH" value="0" /></if>
			
		
		<if advanced="$TH=1"><equip set="TH" /></if> <!--Should always be at the bottom of equips for compatibility -->
		<if advanced="$Main=1"><equip set="Main" /></if><!--Should always be at the bottom of equips for compatibility -->
		
